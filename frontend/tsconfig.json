{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "module": "es6",
        "paths": {
            "@assets/*": ["./src/assets/*"],
            "@components/*": ["./src/components/*"],
            "@constants/*": ["./src/constants/*"],
            "@contexts/*": ["./src/contexts/*"],
            "@hooks/*": ["./src/hooks/*"],
            "@navigation/*": ["./src/navigation/*"],
            "@pages/*": ["./src/pages/*"],
            "@styles/*": ["./src/styles/*"],
            "@types/*": ["./src/types/*"]
        }
    }
}
